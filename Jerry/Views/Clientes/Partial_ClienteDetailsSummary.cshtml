@model Jerry.Models.Cliente

<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Detalles del Cliente</h3>
    </div>
    <div class="panel-body">
        <div>
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.nombreCompleto)
                <p class="form-control-static">@Model.nombreCompleto</p>
            </div>
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.email)
                <p class="form-control-static">@Model.email</p>
            </div>
            <div class="form-group col-md-4">
                @Html.LabelFor(model => model.telefono)
                <p class="form-control-static">@Model.telefono</p>
            </div>
        </div>
        <a href="@Url.Action("Edit","Clientes",new { id = Model.clienteID })">
            <i class="fa fa-pencil"></i><span>Editar</span>
        </a>
    </div>
</div>
